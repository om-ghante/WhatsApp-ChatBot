{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/",
      "dest": "/public/index.html",
      "methods": ["GET"]
    }
  ]
}