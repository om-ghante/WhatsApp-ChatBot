{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "methods": ["GET", "POST"],
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/api/message",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/message"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}